% box dimension definition %
beginadjchar("B",5,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space*0.35u#,space*0.25u#);

% construction %
x1=x2=x3=leftstemloc;
x4=x7=x10;
x5=x6=x9=x8=w-x1;
y1=y10=0;
y3=y4=h;
y9=bar1;
y8=bar2;
y2=y7=bar3;
y6=bar4;
y5=bar5;
z7=z6+whatever*dir(ang); 

% draw %
draw z1--z3--z4--z5--z6--z7--z2 & z2--z7--z8--z9--z10--cycle;

% composite glyphs %
charanchortops_[charcode]=(.5w,h+1/6cap);

penlabels(range 0 thru 30);

endchar;


